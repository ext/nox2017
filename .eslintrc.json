{
  "parserOptions": {
    "ecmaVersion": "2017",
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "sidvind"
  ],
  "rules": {
    "no-console": 0,
    "no-warning-comments": 0
  },
  "overrides": [
    {
      "files": [
        "**/*.ts"
      ],
      "rules" : {
        "no-undef": 0,
        "space-before-blocks": 0
      }
    }
  ]
}
